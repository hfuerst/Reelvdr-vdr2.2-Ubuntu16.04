REMUX_TYPE="remux_convert"  #~Remux mode~F~5~ ~
AUDIO_SOURCE="mp2"         #~Audio source~F~5~ ~
AUDIO_BITRATE="96"         #~Audio bitrate~I~3~64,256~
VIDEO_BITRATE="600"        #~Video bitrate~I~5~100,2000~
AUDIO_CODEC="mp3lame"      #~Audio codec~F~256~a-zA-Z0-9_-~
AUDIO_OPTS="-lameopts preset=${AUDIO_BITRATE}"  #~Audio parms~F~256~a-zA-Z0-9_-~
OUTPUT_OPTS="-of avi"      #~Output parms~F~256~a-zA-Z0-9_-~
OTHER_OPTS="-vf scale=320:240"  #~Other parms~F~256~a-zA-Z0-9_-:~
VIDEO_CODEC="xvid"         #~Video codec~F~256~a-zA-Z0-9_-~
VIDEO_OPTS_P1="-xvidencopts threads=2:bitrate=${VIDEO_BITRATE}"  #~Video parms~F~256~a-zA-Z0-9_-~
